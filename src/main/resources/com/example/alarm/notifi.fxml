<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="254.0" prefWidth="422.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.alarm.Notifcontrol">
    <children>
        <Button fx:id="taken" layoutX="222.0" layoutY="218.0" mnemonicParsing="false" onAction="#onTakenPressed" opacity="0.73" prefHeight="62.0" prefWidth="218.0" style="-fx-border-color: white; -fx-background-color: green;" text="Taken" textFill="#fffdfd">
            <font>
                <Font size="23.0" />
            </font>
        </Button>
        <Button fx:id="stop" layoutY="218.0" mnemonicParsing="false" opacity="0.66" prefHeight="62.0" prefWidth="218.0" style="-fx-border-color: white; -fx-background-color: red;" text="Stop Alarm" textFill="#fffdfd">
            <font>
                <Font size="23.0" />
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="152.0" opacity="0.47" prefHeight="40.0" prefWidth="135.0" text="ALARM!">
            <font>
                <Font name="Arial Rounded MT Bold" size="33.0" />
            </font>
        </Label>
        <Label fx:id="timez" alignment="CENTER" layoutX="14.0" layoutY="45.0" prefHeight="93.0" prefWidth="174.0" text="Time">
            <font>
                <Font size="40.0" />
            </font>
        </Label>
        <Label fx:id="medname" alignment="CENTER" layoutX="11.0" layoutY="142.0" prefHeight="54.0" prefWidth="174.0" text="Medicine">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Label fx:id="doseg" alignment="CENTER" layoutX="251.0" layoutY="142.0" prefHeight="54.0" prefWidth="174.0" text="Dosage">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <ImageView fitHeight="62.0" fitWidth="62.0" layoutX="331.0" layoutY="61.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@icons8-alarm-clock.gif" />
            </image></ImageView>
    </children>
</AnchorPane>

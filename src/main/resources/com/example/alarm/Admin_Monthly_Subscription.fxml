            </items>
        </ComboBox>
        <TableView fx:id="monthlySubscriptionTable" layoutX="14.0" layoutY="57.0" prefHeight="414.0" prefWidth="695.0">
            <columns>
                <TableColumn fx:id="userID" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="userNameColumn" prefWidth="75.0" text="User" />

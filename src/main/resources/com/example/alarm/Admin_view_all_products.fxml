<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.alarm.AdminShowAllProducts">
    <ScrollPane fitToHeight="true" fitToWidth="true">
        <content>
            <VBox fx:id="itemContainer" prefHeight="480.0" prefWidth="720.0" spacing="50">
            <children>
               <AnchorPane prefHeight="66.0" prefWidth="721.0">
                  <children>
                     <ImageView fitHeight="49.0" fitWidth="71.0" layoutX="18.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true" />
                     <Button layoutX="18.0" layoutY="15.0" mnemonicParsing="false" onAction="#Switch_To_admin_Menu" opacity="0.0" prefHeight="38.0" prefWidth="61.0" text="Back">
                        <cursor>
                           <Cursor fx:constant="HAND" />
                        </cursor></Button>
                     <Button fx:id="Cart" layoutX="23.0" layoutY="16.0" mnemonicParsing="false" onAction="#Switch_To_admin_Menu" opacity="0.65" style="-fx-border-color: #cc7b79; -fx-background-color: #cc7b79; -fx-border-radius: 30; -fx-background-radius: 30;">
                        <graphic>
                           <ImageView fitHeight="32.0" fitWidth="41.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@backfromrem.png" />
                              </image>
                           </ImageView>
                        </graphic>
                     </Button>
                  </children>
               </AnchorPane>
            </children></VBox>
        </content>
    </ScrollPane>
</VBox>
